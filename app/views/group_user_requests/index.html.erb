<p id="notice"><%= notice %></p>

<h1>Group User Requests</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @group_user_requests.each do |group_user_request| %>
      <tr>
        <td><%= link_to 'Show', group_user_request %></td>
        <td><%= link_to 'Edit', edit_group_user_request_path(group_user_request) %></td>
        <td><%= link_to 'Destroy', group_user_request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Group User Request', new_group_user_request_path %>
